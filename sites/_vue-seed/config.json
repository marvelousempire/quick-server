{
  "site": {
    "name": "vue-seed",
    "title": "Vue.js Seed",
    "description": "Seed template for creating Vue.js-based sites with BIS framework integration",
    "version": "1.0.0",
    "type": "vue",
    "themeColor": "#42b983",
    "brand": "#42b983",
    "brand2": "#35495e",
    "accent": "#4fc08d",
    "deployment": {
      "docker": {
        "enabled": true,
        "composeFile": "docker-compose.yml",
        "services": ["vue", "vite"]
      },
      "caddy": {
        "enabled": true,
        "configFile": "Caddyfile"
      },
      "traefik": {
        "enabled": true,
        "labels": {
          "traefik.enable": "true",
          "traefik.http.routers.vue.rule": "Host(`vue.local`)"
        }
      },
      "vite": {
        "enabled": true,
        "hmr": true,
        "port": 5173
      }
    }
  },
